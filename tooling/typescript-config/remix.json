{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Remix.js",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["es2022", "dom", "DOM.Iterable"],
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowJs": true,
    "jsx": "react-jsx",
    "noEmit": true
  }
}
